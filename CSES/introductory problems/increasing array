#include <iostream>
#include <bits/stdc++.h>
#include <string.h>
#include <string>
#include <set>
#include <ctype.h>
#include <algorithm>
#include <vector>
#include <map>
#include <math.h>
typedef long long lld;
using namespace std;

int main(){
	cin.tie(0);
	ios::sync_with_stdio(0);
	lld n,c; cin >>n;
	if(n==1){
		cout <<1;
	}
	else if (n<4){
		cout << "NO SOLUTION";
	}
	else{
		for(int i =2;i>0;i--){
			c=i;
			cout <<c << " ";
			while(c<n-1){
				c+=2;
				cout << c<< " ";
			}
		}
	}
}
