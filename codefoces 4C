#include <iostream>
#include <bits/stdc++.h>
#include <string.h>
#include <string>
#include <set>
#include <ctype.h>
#include <algorithm>
#include <vector>
#include <map>
using namespace std;


int main(){
	ios::sync_with_stdio(0);
	cin.tie(0);
	map <string,int> group;
	int n,i;cin>>n;
	string s;
	while(n--){
		cin >>s;
		if(group.count(s)==0){
			cout << "OK"<<endl;
			group[s]=1;
		}
		else{
			cout <<s<<group[s]<<endl;
			group[s]++;
		}
	}



}
