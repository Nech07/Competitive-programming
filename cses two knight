#include <iostream>
#include <bits/stdc++.h>
#include <string.h>
#include <string>
#include <set>
#include <ctype.h>
#include <algorithm>
#include <vector>
#include <map>
#include <math.h>
#define ll long long
using namespace std;

int main(){
	cin.tie(0);
	ios::sync_with_stdio(0);
	ll n,i; cin >> n;
	for(i=1;i<=n;i++){
		ll totalpositions = i*i*i*(i-1)/2;
		ll attackpositions = 4*(i-1)*(i-2);
		cout<< totalpositions-attackpositions<<endl



			;
	}


}
